{
  "name": "pepper.com",
  "version": "0.0.5",
  "description": "Competence test for Front-end Developer position",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --progress --colors --env=prod",
    "postbuild": "gulp build",
    "clean": "rimraf ./dest ./app/css",
    "lint": "eslint --fix ./app",
    "presetup": "rm -rf ./node_modules",
    "setup": "npm i --silent --no-optional > /dev/null",
    "prestart": "npm run build",
    "start": "static-server ./dest",
    "prewatch": "npm run clean",
    "watch": "concurrently \"cult -- watch\" \"webpack-dev-server\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "https://github.com/tvardy",
  "license": "WTFPL",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cult": "2.0.6",
    "eslint": "4.5.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-css-inline-assets": "0.1.5",
    "gulp-cssnano": "2.1.2",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.1",
    "rimraf": "2.6.1",
    "static-server": "2.0.5",
    "uuid": "3.1.0",
    "vue": "2.4.2",
    "vue-loader": "13.0.4",
    "vue-template-compiler": "2.4.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "4.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "private": true
